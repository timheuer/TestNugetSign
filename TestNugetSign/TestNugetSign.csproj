<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageId>TestNugetSignPackage</PackageId>
    <Authors>Tim Heuer</Authors>
    <Company>Tim Heuer</Company>
    <Product>TestNugetSign</Product>
    <Description>A sample library</Description>
    <Copyright>2021, All rights reserved</Copyright>
    <PackageProjectUrl>https://github.com/timheuer/TestNugetSign</PackageProjectUrl>
    <RepositoryUrl>https://github.com/timheuer/TestNugetSign</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <DebugType>embedded</DebugType>
    <FileVersion>1.1.0</FileVersion>
    <AssemblyVersion>1.1.0</AssemblyVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true'">
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>
</Project>
